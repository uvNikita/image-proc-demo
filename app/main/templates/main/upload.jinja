{% extends "base.jinja" %}
{% set page_title = 'Upload image' %}

{% block content %}
  <h3>Upload new File</h3>
  <p>
    <img src="{{ image_url }}"/>
  </p>

  <p>
    <form action="" method=post enctype=multipart/form-data>
      <p>
        <input type=file name=file>
        <input type=submit value=Upload>
      </p>
    </form>
  </p>
{% endblock %}
